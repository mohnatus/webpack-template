{
  "name": "webpack-start",
  "version": "1.1.0",
  "description": "",
  "main": "src/index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run eslint:fix",
      "git add"
    ],
    "*.scss": [
      "npm run stylelint:fix",
      "git add"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint src --ext .js,.jsx",
    "eslint:fix": "eslint src --fix --ext .js,.jsx",
    "stylelint": "stylelint src/**/*.scss --output-file stylelint.log --syntax scss",
    "stylelint:fix": "stylelint src/**/*.scss --output-file stylelint.log --syntax scss --fix",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "env:prod": "cross-env NODE_ENV=production",
    "env:dev": "cross-env NODE_ENV=development",
    "stats": "webpack --profile --json > stats.json",
    "analyze": "npm run env:prod && npm run stats && webpack-bundle-analyzer stats.json",
    "prod": "npm run env:prod webpack",
    "dev": "npm run env:dev webpack",
    "dev:w": "npm run env:dev webpack --watch",
    "server": "npm run env:dev webpack-dev-server",
    "build": "npm run prod",
    "start": "npm run server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-export-default-from": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chunks-webpack-plugin": "^3.4.5",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "husky": "^4.2.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "prop-types": "^15.7.2",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-webpack-plugin": "^1.2.1",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
